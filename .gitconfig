[alias]
  p = push
  pm = push origin master
  a = add
  c = commit
  d = diff
  co = checkout
  b = branch
  lg = log --oneline --decorate
  s = status --branch --short
  branches = branch -a
  remotes = remote -v
  undo = reset --soft HEAD^
[core]
	excludesfile = ~/.gitignore
[commit]
	gpgsign = true
[rebase]
	autoStash = true
[pull]
	rebase = true
[init]
	defaultBranch = main
[gpg]
	program = gpg2
[url "git@github.com:"]
	insteadOf = https://github.com/
[diff]
	renameLimit = 16384
