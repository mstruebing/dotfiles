ALIASES=~/.zsh/aliases
FUNCTIONS=~/.zsh/functions
EXPORTS=~/.zsh/exports
ANTIGEN=$(brew --prefix)/share/antigen/antigen.zsh
export ASDF_DIR='/usr/local/opt/asdf/libexec'
ASDF=$ASDF_DIR/asdf.sh
Z=$(brew --prefix)/etc/profile.d/z.sh

source $ALIASES
source $FUNCTIONS
source $EXPORTS
source $ANTIGEN
source $ASDF
source $Z
